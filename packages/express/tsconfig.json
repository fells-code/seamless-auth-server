  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "outDir": "dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,

    "declaration": true,
    "emitDeclarationOnly": false,
    "declarationMap": true
  },
  "include": ["src"]
}
